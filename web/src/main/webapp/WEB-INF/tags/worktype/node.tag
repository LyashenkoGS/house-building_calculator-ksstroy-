<%@ tag body-content="empty" trimDirectiveWhitespaces="true" %><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><%@ taglib prefix="spring" uri="http://www.springframework.org/tags" %><%@ taglib prefix="worktype" tagdir="/WEB-INF/tags/worktype" %><%@ attribute name="workTypeGroup" required="true" type="ua.ksstroy.logic.worktype.WorkTypeGroupData" %><ul class="group">    <worktype:workTypeGroup workTypeGroup="${workTypeGroup}"/>     <worktype:addWorkTypeGroup innerWorkTypeGroup="${workTypeGroup}"/>       <worktype:updateWorkTypeGroup workTypeGroup="${workTypeGroup}"/>       <worktype:addWorkTypes workTypeGroup="${workTypeGroup}"/>       <worktype:workTypes workTypchikList="${workTypeGroup.workTypesData}"/>    <ul class="subgroups">        <c:forEach var="innerWorkTypeGroup" items="${workTypeGroup.workTypeGroupsData}">            <worktype:node workTypeGroup="${innerWorkTypeGroup}"/>        </c:forEach>    </ul></ul>