<%@ tag body-content="empty" trimDirectiveWhitespaces="true" %><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><%@ taglib prefix="spring" uri="http://www.springframework.org/tags" %><%@ taglib prefix="worktype" tagdir="/WEB-INF/tags/worktype" %><%@ attribute name="workTypeGroup" required="true" type="ua.ksstroy.logic.worktype.WorkTypeGroupData" %><ul class="group">    <worktype:workTypeGroup workTypeGroup="${workTypeGroup}"/>    <ul class="subgroups">        <worktype:workTypes workTypchikList="${workTypeGroup.workTypesData}"/>        <c:forEach var="innerMaterialType" items="${workTypeGroup.workTypeGroupsData}">            <worktype:node workTypeGroup="${innerMaterialType}"/>        </c:forEach>    </ul></ul>